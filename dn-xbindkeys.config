#! /bin/sh
# debconf script for dn-xbindkeys
#
# see: debconf-devel(7), confmodule(3)

set -e
. /usr/share/debconf/confmodule  # use debconf

# check whether to overwrite or backup existing config files
db_input critical dn-xbindkeys/handle-existing-configs || true
db_go || true

# check whether to install xbindkeys desktop launcher
db_input critical dn-xbindkeys/install-launcher || true
db_go || true
